<SearchPlugin xmlns:os="http://a9.com/-/spec/opensearch/1.1/" xmlns="http://www.mozilla.org/2006/browser/search/">
    <os:ShortName>Wikipedia (nl)</os:ShortName>
    <os:Description>Wikipedia, de vrije encyclopedie</os:Description>
    <os:InputEncoding>UTF-8</os:InputEncoding>
    <os:Image height="16" width="16">data:image/x-icon;base64,</os:Image>
    <os:Image height="64" width="64">data:image/png;base64,</os:Image>
    <SearchForm>http://nl.wikipedia.org/wiki/Speciaal:Zoeken</SearchForm>
    <os:Url method="GET" resultDomain="nl.wikipedia.org" template="http://nl.wikipedia.org/w/api.php" type="application/x-suggestions+json">
        <os:Param name="action" value="opensearch"/>
        <os:Param name="search" value="{searchTerms}"/>
    </os:Url>
    <os:Url method="GET" resultDomain="nl.wikipedia.org" template="https://nl.wikipedia.org/w/index.php" type="text/html">
        <os:Param name="title" value="Speciaal:Zoeken"/>
        <os:Param name="search" value="{searchTerms}"/>
        <os:Param name="sourceid" value="Mozilla-search"/>
    </os:Url>
</SearchPlugin>
